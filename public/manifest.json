{
  "name": "github-readme-stats-extention",
  "description": "github-readme-stats-extention",
  "manifest_version": 2,
  "version": "0.202104.0",
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/16x16.png",
      "48": "icons/48x48.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://github.com/*",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "hot-reload.js",
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}